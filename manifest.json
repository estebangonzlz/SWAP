{
  "manifest_version": 3,
  "name": "URL Redirector",
  "version": "1.0",
  "description": "Automatically redirects URLs based on predefined patterns",
  "permissions": [
    "webNavigation",
    "storage"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_icon": "images/icon48.png"
  },
  "options_page": "options.html",
  "icons": {
    "48": "images/icon48.png"
  }
}
